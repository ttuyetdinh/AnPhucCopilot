{
  "name": "anphuc-copilot",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "prisma:migrate": "dotenv -e .env.local -- pnpm prisma migrate dev",
    "prisma:migrate:create": "dotenv -e .env.local -- pnpm prisma migrate dev --create-only",
    "prisma:migrate:reset": "dotenv -e .env.local -- pnpm prisma migrate reset --force",
    "prisma:migrate:init": "pnpm prisma:migrate:reset && pnpm prisma:migrate init",
    "prisma:generate": "pnpm prisma generate",
    "prisma:studio": "dotenv -e .env.local -- pnpm prisma studio",
    "prisma:seed": "dotenv -e .env.local -- pnpm prisma db seed",
    "prisma:push": "dotenv -e .env.local -- pnpm prisma db push"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.2.1",
    "@ai-sdk/react": "^1.1.21",
    "@clerk/localizations": "^3.13.0",
    "@clerk/nextjs": "^6.12.5",
    "@heroui/react": "^2.7.5",
    "@heroui/theme": "^2.4.12",
    "@langchain/community": "^0.3.34",
    "@langchain/core": "^0.3.42",
    "@langchain/openai": "^0.4.4",
    "@langchain/textsplitters": "^0.1.0",
    "@prisma/client": "latest",
    "@tanstack/react-query": "^5.67.2",
    "ai": "^4.1.54",
    "framer-motion": "^12.6.3",
    "gpt-tokens": "^1.3.14",
    "langchain": "^0.3.19",
    "lucide-react": "^0.487.0",
    "minio": "^8.0.4",
    "next": "15.2.4",
    "pdf-parse": "^1.1.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-dropzone": "^14.3.8",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "zod": "^3.24.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^20.17.25",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.2.1",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "latest",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "packageManager": "pnpm@10.7.1"
}
